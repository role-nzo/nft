        <div id="popups"></div>
        <script src="js/components/Home.js" type="text/babel"></script>
        <script src="js/components/Verifier.js" type="text/babel"></script>
        <script src="js/components/Popupper.js" type="text/babel"></script>
        <script src="js/components/Popup.js" type="text/babel"></script>
        
        <script type="text/babel">
            ReactDOM.render(
                <React.StrictMode>
                    <Popupper ref={popupper}/>
